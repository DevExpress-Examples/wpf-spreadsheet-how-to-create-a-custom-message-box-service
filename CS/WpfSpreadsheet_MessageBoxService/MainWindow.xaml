<dx:ThemedWindow x:Class="WpfSpreadsheet_MessageBoxService.MainWindow" mc:Ignorable="d" Title="Spreadsheet" Height="600" Width="800" 
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:local="clr-namespace:WpfSpreadsheet_MessageBoxService" 
    xmlns:dxsps="http://schemas.devexpress.com/winfx/2008/xaml/spreadsheet">
    <Grid>
        <dxsps:SpreadsheetControl x:Name="spreadsheet" CommandBarStyle="Ribbon" ShowFormulaBar="True" Loaded="spreadsheet_Loaded"
                                  DocumentSource="pack://application:,,,/WpfSpreadsheet_MessageBoxService;component/Document.xlsx"/>
    </Grid>
</dx:ThemedWindow>
